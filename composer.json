{
	"name": "qingze-lab/openim-sdk-php",
	"description": "OpenIM REST API PHP SDK with token management, retryable HTTP, and domain services.",
	"type": "library",
	"license": "MIT",
	"homepage": "https://github.com/qingze-lab/openim-php-sdk",
	"keywords": [
		"openim",
		"im",
		"sdk",
		"rest",
		"php"
	],
	"authors": [
		{
			"name": "Qingze Lab",
			"email": "opensource@qingze.lab",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^8.2",
		"guzzlehttp/guzzle": "^7.8",
		"psr/simple-cache": "^3.0"
	},
	"require-dev": {
		"phpunit/phpunit": "^10.5"
	},
	"autoload": {
		"psr-4": {
			"QingzeLab\\OpenIM\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"QingzeLab\\OpenIM\\Tests\\": "tests/"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:qingze-lab/openim-sdk-php.git"
		}
	],
	"minimum-stability": "dev",
	"prefer-stable": true
}
